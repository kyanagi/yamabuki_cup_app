<div class="container mb-6">
  <%= render "admin/shared/breadcrumb", items: [
    { label: "ペーパー採点", path: admin_round1_path },
    { label: "近似値", path: nil }
  ] %>
  <div class="block has-text-right">
    <%= link_to "新規入力", new_admin_round1_approximation_quiz_answer_path %>
  </div>
  <table class="table is-striped is-narrow is-hoverable is-fullwidth is-max-tablet container">
    <thead>
      <tr>
        <th>ID</th>
        <th>氏名</th>
        <th class="has-text-right">近似値1</th>
        <th class="has-text-right">近似値2</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: "player", collection: @players %>
    </tbody>
  </table>
</div>
