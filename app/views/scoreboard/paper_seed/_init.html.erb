<div class="columns-2">
  <div class="paper-seed-container">
    <% 1.upto(7) do |rank|%>
      <div class="paper-seed-player" id="paper-seed-player-<%= rank %>">
        <div class="paper-seed-rank"><%= rank %></div>
        <div class="paper-seed-name"></div>
        <div class="paper-seed-score"></div>
      </div>
    <% end %>
  </div>
  <div id="question"></div>
</div>
